import java.lang.Math;
class Converter{
	public static void main(String a[])
	{
		System.out.println("Binary to Decimal number is "+Integer.parseInt(a[0],2));
		System.out.println("Octal to decimal number is "+Integer.parseInt(a[0],8));
		System.out.println("Hex to Decimal number is "+Integer.parseInt(a[0],16));
	}
}